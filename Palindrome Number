class Palindrome{
    public static void main(String[] args){
       int rev=0;
       int num=454;
       int temp=num;
       int rem;
       while(num>0)
       {
           rem=num%10;
           rev=rev*10+rem;
           num=num/10;
        
       }
       if(temp==rev){
           System.out.println("palindrome");
       }
       else{
           System.out.println("not palindrome");
       }
       
    }
}
