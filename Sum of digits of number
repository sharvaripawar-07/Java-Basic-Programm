import java.util.*;
class Sumofdigit{
    public static void main(String[] args){
        int rem;
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        // int num=2468;
        int sum=0;
        while(num>0){
            rem=num%10;
            sum=sum+rem;
            num=num/10;
        }
        System.out.println("sum of digit is "+sum);
    }
}
